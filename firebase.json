{
  "hosting": {
    "public": "dist/VotationUPC",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "https://electionupc.com/__/auth/action?mode=''&oobCode=''&apiKey=''",
        "destination": "https://electionupc.com/ingresar"
      }
    ]
  }
}
